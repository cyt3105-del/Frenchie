{
  "buildCommand": "rm -rf node_modules/.cache && npx expo export --platform web --clear",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "env": {
    "NODE_ENV": "production",
    "EXPO_USE_METRO_WORKSPACE_ROOT": "1"
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
